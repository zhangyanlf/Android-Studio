<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apple.component">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:label="UI测试">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".simpleComponentActivity"
            android:label="常用简单的Component" />
        <activity android:name=".menuComponentActivity" android:label="菜单"/>
        <activity android:name=".guageComponentActivity" android:label="进度条"/>
        <activity android:name=".DialogComponentActivity" android:label="对话框"></activity>
    </application>

</manifest>